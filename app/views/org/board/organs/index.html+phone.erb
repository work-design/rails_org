<% @organs.each do |organ| %>
  <%= link_to({ controller: '/me/home', host: organ.host }, class: 'weui-cell weui-cell_access') do %>
    <div class="weui-cell__bd"><%= organ.name %></div>
    <div class="weui-cell__ft"></div>
  <% end %>
<% end %>
<% Roled::OrganRole.visible.each do |role| %>
  <%= link_to({ controller: 'organs', action: 'new', role_id: role.id }, class: 'weui-cell weui-cell_access') do %>
    <i class="fa-light fa-plus"></i>
    <span><%= role.name %></span>
  <% end %>
<% end %>
