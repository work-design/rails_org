<%= render 'filter_form' %>
<%= render 'filter_table' %>

<div class="columns">
  <div class="column">
    <table class="table is-hoverable is-fullwidth">
      <thead>
      <tr>
      
      </tr>
      </thead>
    
      <tbody>
      <% @members.each do |member| %>
        <%= render partial: 'member', locals: { member: member } %>
      <% end %>
      </tbody>
    </table>
  
    <%= paginate @members %>
  </div>
  <div class="column is-narrow">
    <%= render 'nav_show' %>
  </div>
</div>
