<div class="buttons are-small is-centered mb-0">
  <%= link_to({ action: 'new' }, class: 'button is-link') do %>
    <i class="fa-solid fa-plus"></i>
    <span class="pl-1"><%= t('.new.title') %></span>
  <% end %>
  <%= link_to({ controller: 'datum/admin/table_lists', action: 'find', data_list_id: 'Org::MemberExport', **params.permit(:organ_id) }, class: 'button is-light') do %>
