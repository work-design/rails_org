<%= form_with model: @tutorial, url: { action: 'create' } do |f| %>
  <%= render 'error_messages', target: @tutorial %>
  <%= f.select :tutor_id, options_for_select([[@tutorial.tutor&.name, @tutorial.tutor_id]], @tutorial.tutor_id) %>
  <%= f.text_field :kind, readonly: true %>
  <%= f.date_field :start_on %>
  <%= f.date_field :finish_on %>
  <%= f.submit %>
<% end %>
