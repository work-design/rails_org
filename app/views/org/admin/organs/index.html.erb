<%= render layout: 'index_tree_table', locals: { cache_key: Organ.column_names.hash } do %>
  <%= render partial: 'organ', object: current_organ %>
  <% @organs.each do |organ| %>
    <%= render partial: 'organ', object: organ %>
    <%= draw_tree(partial: 'organ', object: organ) %>
  <% end %>
<% end %>

<%= paginate @organs %>
