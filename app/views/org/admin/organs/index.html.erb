<%= render layout: 'index_tree_table', locals: { cache_key: Organ.column_names.hash } do %>
  <%= render partial: 'index_tbody', layout: 'index_tr', locals: { model: current_organ } %>
  <% @organs.each do |organ| %>
    <%= render partial: 'index_tbody', layout: 'index_tr', locals: { model: organ } %>
    <%= draw_tree(partial: 'index_tbody', layout: 'index_tr', object: organ, locals: { model: organ }) %>
  <% end %>
<% end %>

<%= paginate @organs %>
