<%= link_to({ controller: 'org/me/members', action: 'show', host: current_client.organ.host, auth_token: current_account.auth_token }, state: 'enter', class: 'weui-cell weui-cell_access') do %>
  <div class="weui-cell__bd">
    <i class="fa-solid fa-fw fa-user has-text-primary"></i>
    <span><%= t('org.me.members.show.title') %></span>
  </div>
  <div class="weui-cell__ft"></div>
<% end %>
<%= link_to({ controller: 'org/me/members', action: 'qrcodes' }, state: 'enter', class: 'weui-cell weui-cell_access') do %>
