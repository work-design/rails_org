<% if rails_role_user.is_a?(Org::Member) %>
  <div data-controller="visit" data-visit-url-value="<%= url_for(controller: '/admin/home') %>"></div>
<% else %>
  <div data-controller="visit" data-visit-url-value="<%= url_for(controller: 'org/board/organs', **params.permit(:role_id)) %>"></div>
<% end %>
